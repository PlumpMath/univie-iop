@prefix s: <https://univie.ac.at/~a7901955/schema/> .
@prefix : <http://univie.ac.at/~a7901955/contents/> .
@prefix rdf: <https://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <https://www.w3.org/2000/01/rdf-schema#>  .
@prefix dc: <https://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# begin schema

s:studybranch 
	s:german "Studienrichtung" ;
	rdfs:subClassOf s:lectureindex ;
    dc:title s:name .
    
s:study 
	s:german "Studium" ;
	rdfs:subClassOf s:studybranch ;
    dc:title s:name .

s:modulegroup
	s:german "Modulgruppe" ;
	rdfs:subClassOf  s:study ;
    dc:title s:name .

s:module 
	s:german "Modul" ;
	rdfs:subClassOf s:modulegroup ;
    dc:title s:name .

s:course
	s:german "Lehrveranstaltung" ;
    s:continousassessment rdf:value ;
    s:ects rdf:value ;
    s:hoursPerWeek rdf:value ;
    dc:subject s:name ;
    s:type rdf:value ;
    s:hasLanguages s:languages ;
    s:hasLecturers s:lecturers .
    
s:group
	s:german "Gruppe" ;
    s:block rdf:value ;
    s:learningplatform rdf:value ;
    s:learningplatformurl rdf:value ;
    s:livestream rdf:value ;
    s:signlanguage rdf:value ;
    s:lang rdf:value ;
    s:lect_responsible rdf:value ;
    s:lect_auxiliary rdf:value .

s:appointment
	s:german "Termin" ;
	rdfs:subClassOf s:group ;
    s:start rdf:value ;
    s:end rdf:value ;
    s:room rdf:value ;
    #s:location rdf:value ;
    #s:street rdf:value ;
    #s:zip rdf:value .
    
# end schema    

    
:st706_2 rdfs:subClassOf  s:study .
