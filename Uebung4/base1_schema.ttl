@prefix s: <https://univie.ac.at/~a7901955/schema/> .
@prefix : <http://univie.ac.at/~a7901955/contents/> .
@prefix rdf: <https://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <https://www.w3.org/2000/01/rdf-schema#>  .
@prefix dc: <https://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# begin schema


s:hasStudy
  a rdf:Property;
  rdfs:label "gruppiert Curricula"@de ;
  rdfs:label "groups studies"@en ;
  rdfs:domain s:Studybranch ;
  rdfs:range s:Study ;
  
s:Studybranch 
  a rdfs:Class ;
  rdfs:Label "Studienrichtung"@de ;
  rdfs:Label "Studybranch"@en ;
    dc:title s:name .
    
s:hasModulegroup
  a rdf:Property;
  rdfs:label "gruppiert Modulgruppen"@de ;
  rdfs:label "groups modulegroup"@en ;
  rdfs:domain s:Study ;
  rdfs:range s:Modulegroup ;
  
s:Study 
  a rdfs:Class ;
  rdfs:Label "Studium"@de ;
  rdfs:Label "Study"@en ;
    rdfs:subClassOf s:studybranch ;
    dc:title s:name .

s:Modulegroup
  a rdfs:Class ;
  rdfs:Label "Modulgruppe"@de ;
  rdfs:Label "Modulegroup"@en ;
    rdfs:subClassOf  s:study ;
    dc:title s:name .

s:Module 
  a rdfs:Class ;
  rdfs:Label "Modul"@de ;
  rdfs:Label "Module"@en ;
    rdfs:subClassOf s:modulegroup ;
    dc:title s:name .

s:Course
  a rdfs:Class ;
  rdfs:Label "Lehrveranstaltung"@de ;
  rdfs:Label "Course"@en ;
  s:continousassessment rdf:value ;
  s:ects rdf:value ;
  s:hoursPerWeek rdf:value ;
  dc:subject s:name ;
  s:type rdf:value ;
    
s:lect_responsible
  a rdf:Property;
  rdfs:label "Verantwortlicher Leiter"@de ;
  rdfs:label "responsible Lecturer"@en ;
  rdfs:domain s:Group ;
  rdfs:range rdf:Value ;
  
s:lect_auxiliary
  a rdf:Property;
  rdfs:label "Mitanbieter"@de ;
  rdfs:label "auxiliary lecturer"@en ;
  rdfs:domain s:Group ;
  rdfs:range rdf:Value ;
  
s:Group
  a rdfs:Class ;
  rdfs:Label "Gruppe"@de ;
  rdfs:Label "Group"@en ;
    s:block rdf:value ;
    s:learningplatform rdf:value ;
    s:learningplatformurl rdf:value ;
    s:livestream rdf:value ;
    s:signlanguage rdf:value ;
    s:lang rdf:value ;
    s:lect_responsible rdf:value ;
    s:lect_auxiliary rdf:value .

s:Appointment
  a rdfs:Class ;
  rdfs:Label "Termin"@de ;
  rdfs:Label "Appointement"@en ;
    s:german "Termin" ;
    rdfs:subClassOf s:group ;
    s:start rdf:value ;
    s:end rdf:value ;
    #s:location rdf:value ;
    #s:street rdf:value ;
    #s:zip rdf:value ;
    s:room rdf:value .
    
# end schema    
